{
  "name": "cbor-x",
  "author": "Kris Zyp",
  "version": "0.8.0",
  "description": "Ultra-fast CBOR implementation with tag extensions for records and structured cloning",
  "license": "MIT",
  "types": "./index.d.ts",
  "keywords": [
    "CBOR",
    "performance",
    "structured",
    "clone"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/kriszyp/cbor-x"
  },
  "scripts": {
    "benchmark": "node ./tests/benchmark.js",
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "./node_modules/.bin/mocha --experimental-json-modules tests/test.js -u tdd"
  },
  "type": "module",
  "main": "./dist/node.cjs",
  "exports": {
    ".": {
      "node": {
        "require": "./dist/node.cjs",
        "import": "./node.js"
      },
      "default": {
        "import": "./index.js"
      }
    },
    "./pack": {
      "node": {
        "import": "./index.js",
        "require": "./dist/node.cjs"
      },
      "default": {
        "import": "./pack.js"
      }
    },
    "./unpack": {
      "node": {
        "import": "./index.js",
        "require": "./dist/node.cjs"
      },
      "default": {
        "import": "./unpack.js"
      }
    }
  },
  "optionalDependencies": {
    "cbor-extract": "^0.2.2"
  },
  "devDependencies": {
    "async": "^3",
    "cbor": "^5",
    "cbor-sync": "^1.0.4",
    "@types/node": "latest",
    "chai": "^4",
    "esm": "^3.2.25",
    "mocha": "^8.1.3",
    "rollup": "^1.20.3",
    "rollup-plugin-babel-minify": "^9.0.0",
    "@rollup/plugin-json": "^4.1.0"
  }
}
