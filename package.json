{
  "name": "cbor-x",
  "author": "Kris Zyp",
  "version": "0.8.3",
  "description": "Ultra-fast CBOR implementation with tag extensions for records and structured cloning",
  "license": "MIT",
  "types": "./index.d.ts",
  "keywords": [
    "CBOR",
    "performance",
    "structured",
    "clone"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/kriszyp/cbor-x"
  },
  "scripts": {
    "benchmark": "node ./tests/benchmark.cjs",
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "mocha tests/test**.*js -u tdd --experimental-json-modules"
  },
  "type": "module",
  "main": "./dist/node.cjs",
  "exports": {
    ".": {
      "node": {
        "require": "./dist/node.cjs",
        "import": "./node-index.js"
      },
      "default": {
        "import": "./index.js"
      }
    },
    "./encode": {
      "node": {
        "import": "./index.js",
        "require": "./dist/node.cjs"
      },
      "default": {
        "import": "./encode.js"
      }
    },
    "./decode": {
      "node": {
        "import": "./index.js",
        "require": "./dist/node.cjs"
      },
      "default": {
        "import": "./decode.js"
      }
    }
  },
  "optionalDependencies": {
    "cbor-extract": "^0.3.1"
  },
  "devDependencies": {
    "@types/node": "latest",
    "async": "^3",
    "cbor": "^5",
    "cbor-sync": "^1.0.4",
    "chai": "^4.3.4",
    "esm": "^3.2.25",
    "mocha": "^8.1.3",
    "rollup": "^1.20.3",
    "rollup-plugin-babel-minify": "^9.0.0"
  }
}
