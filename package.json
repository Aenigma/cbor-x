{
  "name": "msgpackr",
  "author": "Kris Zyp",
  "version": "1.2.10",
  "description": "Ultra-fast MessagePack implementation with extensions for records and structured cloning",
  "license": "MIT",
  "types": "./index.d.ts",
  "keywords": [
    "MessagePack",
    "msgpack",
    "performance",
    "structured",
    "clone"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/kriszyp/msgpackr"
  },
  "scripts": {
    "benchmark": "node ./tests/benchmark.js",
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "./node_modules/.bin/mocha --experimental-json-modules tests/test.js -u tdd"
  },
  "type": "module",
  "main": "./dist/node.cjs",
  "browser": {
    "./index.mjs": "./browser.mjs",
    "./index.js": "./browser.js"
  },
  "exports": {
    ".": {
      "node": {
        "require": "./dist/node.cjs",
        "import": "./node.js"
      },
      "default": {
        "import": "./browser.js"
      }
    },
    "./pack": {
      "node": {
        "import": "./index.js"
      },
      "default": {
        "import": "./pack.js"
      }
    },
    "./unpack": {
      "node": {
        "import": "./index.mjs",
        "require": "./index.js"
      },
      "default": {
        "import": "./unpack.mjs",
        "require": "./unpack.js"
      }
    }
  },
  "optionalDependencies": {
    "msgpackr-extract": "^1.0.8"
  },
  "devDependencies": {
    "@types/node": "latest",
    "chai": "^4",
    "esm": "^3.2.25",
    "mocha": "^8.1.3",
    "rollup": "^1.20.3",
    "rollup-plugin-babel-minify": "^9.0.0"
  }
}
